<header id="header">
	<h1 class="blog-title">
		<a href="<%- config.root %>"><%= config.title %></a>
	</h1>
	<nav class="nav">
		<ul>
			<% for (var i in theme.menu){ %><li><a href="<%- theme.menu[i] %>"><%= i %></a></li><% } %>
			<% if (theme.local_search.enable) { %>
            		<li><%- partial('_partial/search') %>
      			</li><% } %>
		</ul>
	</nav>
</header>
<div id="mobile-div">
	<ul class="mobile-nav-panel">
			<% for (var i in theme.menu){ %><li><a href="<%- theme.menu[i] %>"><%= i %></a></li><% } %>
	</ul>
</div>
